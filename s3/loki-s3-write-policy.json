{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowWriteLogsToLokiBucket",
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:PutObjectAcl"
        ],
        "Resource": "arn:aws:s3:::meryem-loki-logs/*"
      },
      {
        "Sid": "AllowListBucket",
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket"
        ],
        "Resource": "arn:aws:s3:::meryem-loki-logs"
      }
    ]
  }
  